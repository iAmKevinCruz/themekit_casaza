<div class="Container" style="max-width:1200px;">
<div class="about Section--spacingNormal">
    <div class="Grid ArticleListWrapper">
    
      {%- for block in section.blocks -%}
      <div class="Grid__Cell 1/0--phone 1/2--tablet 1/3--lap-and-up">
        <div class="about-box">
          <p>{{block.settings.subtitle}}</p>
          <h2 class="SectionHeader__Heading Heading u-h1">{{block.settings.title}}</h2>
        </div>
      </div>
      {% endfor %}
  </div>
</div>
</div>
{% schema %}
{
  "name": "About ",
  "settings": [],
  "blocks": [
    {
      "type": "image",
      "name": "Image",
      "settings": [
        {
          "type":"text",
          "id":"subtitle",
          "label":"Sub title"
		},
		{
          "type":"text",
          "id":"title",
          "label":"title"
		}
      ]
    }
  ]
}
{% endschema %}